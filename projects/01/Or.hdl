 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 * Not, And
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    And(a=a, b=a, out=out1);
    And(a=b, b=b, out=out2); // どっちか 1 ならOK
    Not(in=out1, out=out3);
    Not(in=out2, out=out4);
    Nand(a=out3, b=out4, out=out);
}
